<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zsgf.platform.mapper.collect.CollectRecordMapper">

    <resultMap type="com.zsgf.platform.model.entity.collect.CollectRecord" id="CollectRecordResult">
        <result property="id" column="id"/>
        <result property="companyId" column="company_id"/>
        <result property="type" column="type"/>
        <result property="deptId" column="dept_id"/>
        <result property="deptName" column="dept_name"/>
        <result property="wasteName" column="waste_name"/>
        <result property="wasteType" column="waste_type"/>
        <result property="wasteCode" column="waste_code"/>
        <result property="weight" column="weight"/>
        <result property="unit" column="unit"/>
        <result property="agentPerson" column="agent_person"/>
        <result property="collectNo" column="collect_no"/>
        <result property="collectPerson" column="collect_person"/>
        <result property="collectTime" column="collect_time"/>
        <result property="whereabouts" column="whereabouts"/>
        <result property="status" column="status"/>
        <result property="signPic" column="sign_pic"/>
        <result property="deleted" column="deleted"/>
    </resultMap>

    <sql id="selectCollectRecordVo">
        select id,
               company_id,
               type,
               dept_id,
               dept_name,
               waste_name,
               waste_type,
               waste_code,
               weight,
               unit,
               agent_person,
               collect_no,
               collect_person,
               collect_time,
               whereabouts,
               status,
               sign_pic,
               deleted
        from tbl_collect_record
    </sql>

</mapper>