<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sjz.governance.mapper.event.AlarmEventFlowNodeMapper">
    
    <resultMap type="com.sjz.governance.model.entity.event.AlarmEventFlowNode" id="AlarmEventFlowNodeResult">
        <result property="id"    column="id"    />
        <result property="dealPerson"    column="deal_person"    />
        <result property="dealExplain"    column="deal_explain"    />
        <result property="dealNode"    column="deal_node"    />
        <result property="dealStatus"    column="deal_status"    />
        <result property="dealFileUrl"    column="deal_file_url"    />
        <result property="eventId"    column="event_id"    />
        <result property="deleted"    column="deleted"    />
    </resultMap>

    <sql id="selectAlarmEventFlowNodeVo">
        select id, deal_person, deal_explain, deal_node, deal_status, deal_file_url, deleted,event_id from ct_alarm_event_flow_node
    </sql>
    <delete id="deleteByAlarmEventId" parameterType="int">
      UPDATE sjz_business.ct_alarm_event_flow_node
        SET deleted=1
        WHERE event_id = #{eventId}
    </delete>
</mapper>