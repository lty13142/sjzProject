<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.crcm.admin.mapper.GatewayPluginsMapper">
    
    <resultMap type="com.crcm.admin.model.entity.GatewayPlugins" id="GatewayPluginsResult">
        <result property="id"    column="id"    />
        <result property="compName"    column="comp_name"    />
        <result property="compFilterName"    column="comp_filter_name"    />
        <result property="compType"    column="comp_type"    />
        <result property="compOrder"    column="comp_order"    />
        <result property="compDesc"    column="comp_desc"    />
        <result property="compMethod"    column="comp_method"    />
        <result property="deleted"    column="deleted"    />
        <result property="enabled"    column="enabled"    />
    </resultMap>

    <sql id="selectGatewayPluginsVo">
        select id, comp_name, comp_filter_name, comp_type, comp_order, comp_desc, comp_method, deleted, enabled, from t_gateway_plugins
    </sql>

</mapper>