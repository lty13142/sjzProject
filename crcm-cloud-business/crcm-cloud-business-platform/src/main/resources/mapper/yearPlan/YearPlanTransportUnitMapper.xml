        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zsgf.platform.mapper.yearPlan.YearPlanTransportUnitMapper">

    <resultMap type="com.zsgf.platform.model.entity.yearPlan.YearPlanTransportUnit" id="YearPlanTransportUnitResult">
        <result property="id" column="ID"/>
        <result property="mainid" column="MAINID"/>
        <result property="ysdwmc" column="YSDWMC"/>
        <result property="ysdwyszz" column="YSDWYSZZ"/>
