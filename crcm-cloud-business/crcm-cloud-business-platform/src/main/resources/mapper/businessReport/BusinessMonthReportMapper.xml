<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zsgf.platform.mapper.businessReport.BusinessMonthReportMapper">

    <resultMap type="com.zsgf.platform.model.entity.businessReport.BusinessMonthReport" id="BusinessMonthReportResult">
        <result property="id" column="ID"/>
        <result property="dwid" column="DWID"/>
        <result property="sbrq" column="SBRQ"/>
        <result property="jynf" column="JYNF"/>
        <result property="jyyf" column="JYYF"/>
        <result property="ssyxts" column="SSYXTS"/>
        <result property="txr" column="TXR"/>
        <result property="txrphone" column="TXRPHONE"/>
        <result property="bz" column="BZ"/>
        <result property="state" column="STATE"/>
        <result property="sfsb" column="SFSB"/>
        <result property="ssshi" column="SSSHI"/>
        <result property="ssqu" column="SSQU"/>
        <result property="modifyrq" column="MODIFYRQ"/>
    </resultMap>

    <sql id="selectBusinessMonthReportVo">
        select ID,
               DWID,
               SBRQ,
               JYNF,
               JYYF,
               SSYXTS,
               TXR,
               TXRPHONE,
               BZ,
               STATE,
               SFSB,
               SSSHI,
               SSQU,
               MODIFYRQ,
        from pp_wxfw_business_month_report
    </sql>

</mapper>