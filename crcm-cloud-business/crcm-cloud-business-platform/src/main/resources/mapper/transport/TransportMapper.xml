<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zsgf.platform.mapper.transport.TransportMapper">

    <resultMap type="com.zsgf.platform.model.entity.transport.Transport" id="TransportResult">
        <result property="id" column="id"/>
        <result property="companyId" column="company_id"/>
        <result property="transportType" column="transport_type"/>
        <result property="transportNo" column="transport_no"/>
        <result property="driverName" column="driver_name"/>
        <result property="driverPhone" column="driver_phone"/>
        <result property="plateNumber" column="plate_number"/>
        <result property="totalWeight" column="total_weight"/>
        <result property="transportStartPlace" column="transport_start_place"/>
        <result property="transportEndPlace" column="transport_end_place"/>
        <result property="transportStartTime" column="transport_start_time"/>
        <result property="transportEndTime" column="transport_end_time"/>
        <result property="status" column="status"/>
        <result property="remark" column="remark"/>
        <result property="driverStatus" column="driver_status"/>
        <result property="finishTime" column="finish_time"/>
        <result property="deleted" column="deleted"/>
    </resultMap>

    <sql id="selectTransportVo">
        select id,
               company_id,
               transport_type,
               transport_no,
               driver_name,
               driver_phone,
               plate_number,
               total_weight,
               transport_start_place,
               transport_end_place,
               transport_start_time,
               transport_end_time,
               status,
               remark,
               driver_status,
               finish_time,
               deleted,
        from tbl_transport
    </sql>

</mapper>